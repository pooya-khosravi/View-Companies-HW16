<div id="myTable">
    <table class="table table-striped table-dark">
        <tr>
            <th scope="col">ردیف</th>
            <th scope="col">نام</th>
            <th scope="col">نام خانوادگی</th>
            <th scope="col">کد ملی</th>
            <th scope="col">جنسیت</th>
            <th scope="col">وضعیت</th>
            <th scope="col">تاریخ تولد</th>
            <th scope="col">نام کمپانی</th>
            <th scope="col">ابزار</th>
        </tr>
        <% for( let i = 0; i < personnel.length; i++ ) { %>
            <tr>
                <th scope="col"> <%= (i+1) %>  </th>
                <td> <%= personnel[i].firstName %>  </td>
                <td> <%= personnel[i].lastName %>  </td>
                <td> <%= personnel[i].codeMelli %>  </td>
                <td> <%= personnel[i].gender %>  </td>
                <% if (personnel[i].isManager === true) { %>
                    <td> مدیر  </td>
                <% } else { %>
                    <td> کارمند عادی </td>
                <% } %> 
                <td> <%= personnel[i].tavallod %>  </td>
                <td> <%= personnel[i].company.name %>  </td>
                <td id="icons">
                    <span title="ویرایش">
                        <i class="fas fa-pen hvr edit" id= <%= personnel[i]._id %> ></i>
                    </span>
                    <span title="حذف">
                        <i class="fas fa-trash-alt hvr delete" id= <%= personnel[i]._id %>></i>
                    </span>
                </td>
            </tr>
        <% } %>
    </table>
</div>
